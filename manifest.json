{
	"manifest_version": 2,
	"name": "IndieGala Giveaways AutoEnterer",
	"version": "1.0.0",
	"permissions": [
		"notifications",
		"storage",
		"tabs",
		"https://www.indiegala.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://www.indiegala.com/*"
			],
			"js": [
				"jquery.js",
				"index.js"
			]
		}
	],
	"description": "IndieGala Giveaways AutoEnterer",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_title": "IndieGala Giveaway AutoEnterer",
		"default_popup": "index.html"
	}
}