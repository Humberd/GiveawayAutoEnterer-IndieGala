{
	"manifest_version": 2,
	"name": "IndieGala Giveaways AutoEnterer",
	"version": "1.0.0",
	"permissions": [
		"notifications",
		"storage",
		"tabs",
		"https://www.indiegala.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://www.indiegala.com/giveaways/*"
			],
			"run_at": "document_end",
			"css": [
				"styles.css"
			],
			"js": [
				"jquery.js",
				"Rx.js",
				"index.js"
			]
		}
	],
	"description": "IndieGala Giveaways AutoEnterer",
	"background": {
		"scripts": [
			"hot-reload.js",
			"Rx.js",
			"background.js"
		]
	},
	"browser_action": {
		"default_title": "IndieGala Giveaway AutoEnterer"
	}
}